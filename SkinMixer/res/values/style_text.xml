<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="low_naked">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="low_center_naked" parent="@style/low_naked">
        <item name="android:gravity">center</item>
    </style>

    <style name="low_orange_naked" parent="@style/low_naked">
        <item name="android:textColor">#E56717</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="low_center_background" parent="@style/low_center_naked">
        <item name="android:background">@drawable/view_tv_unselected_2</item>
    </style>

    <style name="low_background" parent="@style/low_naked">
        <item name="android:background">@drawable/view_tv_unselected_2</item>
    </style>

    <style name="noskin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/view_noskin</item>
        <item name="android:text">@string/skinmixer_noskin_label</item>
    </style>

</resources>